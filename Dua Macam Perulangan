public class Perulangan {
    public static void main(String[] args) {

        int[] age = {12, 4, 5, 2, 6};

        // For
        for (int i = 0; i < age.length; i++)
            System.out.println(age[i]);

        // For-each
        for (int a : age)
            System.out.println(a);
    }
}
